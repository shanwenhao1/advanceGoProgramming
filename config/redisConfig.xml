<redisConfig>
    <!--redis链接地址-->
    <redis_addr>192.168.1.89:6379</redis_addr>
    <!--redis验证密码-->
    <redis_password>123456</redis_password>
    <!--使用的数据库-->
    <redis_db_num>1</redis_db_num>
    <!--连接池大小-->
    <redis_pool_size>30</redis_pool_size>
    <!--空闲连接失效时间, 时间单位是ns(设计实际使用单位为s, 实际使用需 * time.second)-->
    <redis_idle_timeout>120</redis_idle_timeout>
</redisConfig>